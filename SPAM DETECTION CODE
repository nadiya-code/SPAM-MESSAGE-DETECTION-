  {
   "cell_type": "code",
   "execution_count": 125,
   "id": "128997d5-4399-4190-bcd1-8900946dea52",
   "metadata": {},
   "outputs": [],
   "source": [
    "try:\n",
    "    df[\"label_num\"] = df[\"label\"].astype(int)\n",
    "except:\n",
    "    df[\"label_num\"] = df[\"label\"].map({\"ham\": 0, \"spam\": 1})\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "id": "cca7ec79-2549-4b06-b479-4f2c551044f3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def clean_text(text):\n",
    "    text = text.lower()\n",
    "    text = re.sub(r\"http\\S+\", \"\", text)                 # remove URLs\n",
    "    text = text.translate(str.maketrans(\"\", \"\", string.punctuation))  \n",
    "    text = re.sub(r'\\d+', '', text)                    # remove numbers\n",
    "    text = text.strip()\n",
    "    return text"
   ]
  },
